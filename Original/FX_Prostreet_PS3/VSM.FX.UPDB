float4 ShadowCopyPS(VtoP IN) : COLOR0
{
    return tex2D( DIFFUSE_SAMPLER, IN.tex0 );
}

float4 MipMapGenerationPS(VtoP IN) : COLOR0
{
    return tex2D( MIPMAP_GEN_SAMPLER, IN.tex0 );
}


void MipMapGenerationDepthPS(VtoP IN,
						out float4 oColor : COLOR,
                        out float oDepth : DEPTH )
{
    float4 SampledDepth;
    float2 texCoord = IN.tex0;
    asm {
        tfetch2D SampledDepth.x___, texCoord, MIPMAP_GEN_SAMPLER, OffsetX = -0.5, OffsetY = -0.5
        tfetch2D SampledDepth._x__, texCoord, MIPMAP_GEN_SAMPLER, OffsetX =  0.5, OffsetY = -0.5
        tfetch2D SampledDepth.__x_, texCoord, MIPMAP_GEN_SAMPLER, OffsetX = -0.5, OffsetY =  0.5
        tfetch2D SampledDepth.___x, texCoord, MIPMAP_GEN_SAMPLER, OffsetX =  0.5, OffsetY =  0.5
    };
    
    // Find the maximum.
    SampledDepth.xy = max( SampledDepth.xy, SampledDepth.zw );
    SampledDepth.x = max( SampledDepth.x, SampledDepth.y );

	oColor = 0;
//    oDepth = tex2D( MIPMAP_GEN_SAMPLER, IN.tex0 ).x;
	oDepth = SampledDepth.x;
}

float4 DepthToCoeffCosPS(VtoP IN) : COLOR0
{
	float4 OUT;
	
	float pi = 3.14159265;
	float4 c = float4( pi, 3.0*pi, 5.0*pi, 7.0*pi );
	    
	float depth = tex2D( DIFFUSE_SAMPLER, IN.tex0.xy ).x;
	float4 depthScale = c * depth;

	OUT = cos( depthScale )*0.5 + 0.5;
	
	return OUT;
}

float4 DepthToCoeffSinPS(VtoP IN) : COLOR0
{
	float4 OUT;
	
	float pi = 3.14159265;
	float4 c = float4( pi, 3.0*pi, 5.0*pi, 7.0*pi );
	    
	float depth = tex2D( DIFFUSE_SAMPLER, IN.tex0.xy ).x;
	float4 depthScale = c * depth;

	OUT = sin( depthScale )*0.5 + 0.5;
	
	return OUT;
}

float4 EdgeDetectPS(VtoP IN) : COLOR0
{
	float2 tex0 = IN.tex0.xy;

    float4 t0,t1,t2,t3,t4,t5,t6,t7;
    
    asm
    {
		tfetch2D t0.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX = -1.0, OffsetY = -1,MagFilter=point, MinFilter=point
		tfetch2D t1.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX = -1.0, OffsetY = 0,MagFilter=point, MinFilter=point
		tfetch2D t2.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX = -1.0, OffsetY = 1,MagFilter=point, MinFilter=point
		
		tfetch2D t3.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX =  0.0, OffsetY = -1,MagFilter=point, MinFilter=point
		tfetch2D t4.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX =  0.0, OffsetY = 1,MagFilter=point, MinFilter=point
		
		tfetch2D t5.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX =  1.0, OffsetY = -1,MagFilter=point, MinFilter=point
		tfetch2D t6.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX =  1.0, OffsetY = 0,MagFilter=point, MinFilter=point
		tfetch2D t7.xyzw, tex0, DIFFUSE_SAMPLER, OffsetX =  1.0, OffsetY = 1,MagFilter=point, MinFilter=point
    };
    
    float depth0 = ConvertColourToDepth(t0).r * -1 +
					ConvertColourToDepth(t1).r * -2 +
					ConvertColourToDepth(t2).r * -1 +
					ConvertColourToDepth(t3).r * 0 +
					ConvertColourToDepth(t4).r * 0 +
					ConvertColourToDepth(t5).r * 1 +
					ConvertColourToDepth(t6).r * 2 +
					ConvertColourToDepth(t7).r * 1;

    float depth1 = ConvertColourToDepth(t0).r * -1 +
					ConvertColourToDepth(t1).r * 0 +
					ConvertColourToDepth(t2).r * 1 +
					ConvertColourToDepth(t3).r * -2 +
					ConvertColourToDepth(t4).r * 2 +
					ConvertColourToDepth(t5).r * -1 +
					ConvertColourToDepth(t6).r * 0 +
					ConvertColourToDepth(t7).r * 1;
					
    
    return float4(0,0, saturate(abs(depth0) + abs(depth1)) , 1);
}

