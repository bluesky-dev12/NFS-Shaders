//--------------------------------------------------------------------------------------
// Name: VerticalBlurDepthToVariancePS()
// Desc: Vertical portion of a two-pass separable 5x5 Gaussian blur for variance
//       shadow maps
//--------------------------------------------------------------------------------------
float4 VerticalBlurDepthToVariancePS(VtoP IN) : COLOR0
{
	float2 tex0 = IN.tex0.xy;

    // Note that this second pass of the separable filter can use filtered fetches
    // Fetch 4 samples which filter across a column of 5 pixels from the VSM
    float4 t0, t1,t2;
    
#if 1    
    asm
    {
        tfetch2D t0.xy__, tex0, DIFFUSE_SAMPLER, OffsetY = +1.5, MinFilter=linear, MagFilter=linear
        tfetch2D t0.__xy, tex0, DIFFUSE_SAMPLER, OffsetY = +0.5, MinFilter=linear, MagFilter=linear
        tfetch2D t1.xy__, tex0, DIFFUSE_SAMPLER, OffsetY = -0.5, MinFilter=linear, MagFilter=linear
        tfetch2D t1.__xy, tex0, DIFFUSE_SAMPLER, OffsetY = -1.5, MinFilter=linear, MagFilter=linear
        
    };
    // Sum results with Gaussian weights
    float z  = dot( float4( t0.x, t0.z, t1.x, t1.z ), float4( 1.0/4, 1.0/16, 6.0/16, 2.0/16 ) );
    float z2 = dot( float4( t0.y, t0.w, t1.y, t1.w ), float4( 1.0/4, 1.0/16, 6.0/16, 2.0/16 ) );
    
#else
    asm
    {
        tfetch2D t0.xy__, tex0, DIFFUSE_SAMPLER, OffsetY = +2.0, MinFilter=linear, MagFilter=linear
        tfetch2D t0.__xy, tex0, DIFFUSE_SAMPLER, OffsetY = +1.0, MinFilter=linear, MagFilter=linear
        tfetch2D t1.xy__, tex0, DIFFUSE_SAMPLER, OffsetY = -1.0, MinFilter=linear, MagFilter=linear
        tfetch2D t1.__xy, tex0, DIFFUSE_SAMPLER, OffsetY = -2.0, MinFilter=linear, MagFilter=linear
        tfetch2D t2.xy__, tex0, DIFFUSE_SAMPLER, OffsetY = 0, MinFilter=linear, MagFilter=linear
    };
    
    // Sum results with Gaussian weights
    float z  = dot( float4( t0.x, t0.z, t1.x, t1.z ), float4( 1.0/5, 1.0/5, 1.0/5, 1.0/5 ) ) + t2.x * 1.0/5;
    float z2 = dot( float4( t0.y, t0.w, t1.y, t1.w ), float4( 1.0/5, 1.0/5, 1.0/5, 1.0/5 ) ) + t2.y * 1.0/5;
    
#endif    
    return float4( z, z2, 0, 0 );
}

//--------------------------------------------------------------------------------------
// Name: HorizontalBlurBluePS()
//--------------------------------------------------------------------------------------
float4 HorizontalBlurBluePS0(VtoP IN) : COLOR0
{
	float2 tex0 = IN.tex0.xy;

    // Note that this second pass of the separable filter can use filtered fetches
    // Fetch 4 samples which filter across a column of 5 pixels from the VSM
    float4 t0, t1 = 0;
    
    
    asm
    {
        tfetch2D t0.b___, tex0, DIFFUSE_SAMPLER, OffsetX = -3.0, MinFilter=linear, MagFilter=linear
        tfetch2D t0._b__, tex0, DIFFUSE_SAMPLER, OffsetX = -1.5, MinFilter=linear, MagFilter=linear
        tfetch2D t0.__b_, tex0, DIFFUSE_SAMPLER, OffsetX =  1.5, MinFilter=linear, MagFilter=linear
        tfetch2D t0.___b, tex0, DIFFUSE_SAMPLER, OffsetX =  3.0, MinFilter=linear, MagFilter=linear
        tfetch2D t1.rgba, tex0, DIFFUSE_SAMPLER, OffsetX =  0.0, MinFilter=linear, MagFilter=linear
	};
	
	t0.xyz = float3(1.0f,1.0f,1.0f) - t0.xyz;
	t1.z = 1.0f - t1.z;
	
//	float d  = dot( t0.xyzw,  float4( 1.0/16, 4.0/16, 4.0/16, 1.0/16 ) ) + t1.z * ( 6.0 / 16 );

	float d;
	
	if (t1.z &lt; 0.5f)
		d  = dot( t0.xyzw,  float4( 1.0/4, 1.0/4, 1.0/4, 1.0/4 ) );// + t1.z * ( 1.0 / 5 );
	else
		d = t1.z;

	return float4(t1.r,t1.g, saturate(d), t1.a);    

//	return d;
}

