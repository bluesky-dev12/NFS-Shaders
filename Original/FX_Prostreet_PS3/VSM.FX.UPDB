sampler DIFFUSE_SAMPLER = sampler_state
{
	AddressU = CLAMP;
	AddressV = CLAMP;
	MIPFILTER = POINT;
	MINFILTER = POINT;//LINEAR;
	MAGFILTER = POINT;//LINEAR;
};

sampler MIPMAP_GEN_SAMPLER = sampler_state
{
	AddressU = CLAMP;
	AddressV = CLAMP;
	MIPFILTER = POINT;
	MINFILTER = POINT;//LINEAR;
	MAGFILTER = POINT;//LINEAR;
};

struct VS_INPUT_SCREEN
{
	float4 position	: POSITION;
	float4 tex0		: TEXCOORD0;
};

struct VtoP
{
	float4 position		: POSITION;
	float4 tex0			: TEXCOORD0;
};

VtoP VertexShaderPassThrough(const VS_INPUT_SCREEN IN)
{
	VtoP OUT;
	OUT.position = IN.position;// + float4(0.5/1600, 0.5/1600,0,0);
	OUT.tex0 = IN.tex0;
	return OUT;
}

float4 ConvertColourToDepth(float4 input)
{
	float4 result;
	
	result.arg = input.arg * 255.0f;

	//result.argb = round(result.argb);

	float depth = dot(result.arg, float3(65536, 256, 1));

	depth *= 1.0f / 16777215.0f;

	result.arg = depth;
	
	result.b = input.b;
	
	return result;
}

//--------------------------------------------------------------------------------------
// Name: DepthToVariancePS()
// Desc: Copy depth to variance
//--------------------------------------------------------------------------------------
float4 DepthToVariancePS(VtoP IN) : COLOR0
{
	float4 base = tex2D(DIFFUSE_SAMPLER, IN.tex0.xy);
	return float4(base.x, base.x * base.x, 0, 0);
}


//--------------------------------------------------------------------------------------
// Name: HorizontalBlurDepthToVariancePS()
// Desc: Copy depth to variance and perform the horizontal portion of a two-pass
//       separable 5x5 Gaussian blur
//--------------------------------------------------------------------------------------
float4 HorizontalBlurDepthToVariancePS(VtoP IN) : COLOR0
{
	float2 tex0 = IN.tex0.xy;

    // Fetch a row of 5 pixels from the D24S8 depth map
    float4 DepthSamples0123;
    float4 DepthSamples4___;
#if 1    
    asm
    {
        tfetch2D DepthSamples0123.x___, tex0, DIFFUSE_SAMPLER, OffsetX = -2.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123._x__, tex0, DIFFUSE_SAMPLER, OffsetX = -1.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123.__x_, tex0, DIFFUSE_SAMPLER, OffsetX = -0.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123.___x, tex0, DIFFUSE_SAMPLER, OffsetX = +1.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples4___.x___, tex0, DIFFUSE_SAMPLER, OffsetX = +2.0, MinFilter=point, MagFilter=point
    };
    
    // Do the Guassian blur (using a 5-tap filter kernel of [ 1 4 6 4 1 ] )
    float z  = dot( DepthSamples0123.xyzw,  float4( 1.0/16, 4.0/16, 6.0/16, 4.0/16 ) ) + DepthSamples4___.x * ( 1.0 / 16 );

    DepthSamples0123.xyzw = DepthSamples0123.xyzw * DepthSamples0123.xyzw;
    DepthSamples4___.x    = DepthSamples4___.x    * DepthSamples4___.x;
    float z2 = dot( DepthSamples0123.xyzw,  float4( 1.0/16, 4.0/16, 6.0/16, 4.0/16 ) ) + DepthSamples4___.x * ( 1.0 / 16 );
#else
    asm
    {
        tfetch2D DepthSamples0123.x___, tex0, DIFFUSE_SAMPLER, OffsetX = -2.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123._x__, tex0, DIFFUSE_SAMPLER, OffsetX = -1.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123.__x_, tex0, DIFFUSE_SAMPLER, OffsetX = +1.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples0123.___x, tex0, DIFFUSE_SAMPLER, OffsetX = +2.0, MinFilter=point, MagFilter=point
        tfetch2D DepthSamples4___.x___, tex0, DIFFUSE_SAMPLER, OffsetX = 0.0, MinFilter=point, MagFilter=point
    };
    
    // Do the Guassian blur (using a 5-tap filter kernel of [ 1 4 6 4 1 ] )
    float z  = dot( DepthSamples0123.xyzw,  float4( 1.5/5, 0.6/5, 0.6/5, 1.5/5 ) ) + DepthSamples4___.x * ( 0.6 / 5 );

    DepthSamples0123.xyzw = DepthSamples0123.xyzw * DepthSamples0123.xyzw;
    DepthSamples4___.x    = DepthSamples4___.x    * DepthSamples4___.x;
    float z2 = dot( DepthSamples0123.xyzw,  float4( 1.5/5, 0.6/5, 0.6/5, 1.5/5 ) ) + DepthSamples4___.x * ( 0.6 / 5 );
#endif	
    return float4( z, z2, 0, 0 );
}

