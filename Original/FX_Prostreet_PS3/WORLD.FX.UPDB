#ifndef WORLD_H
#define WORLD_H

#include "global.h"
#include "fog.h"
#include "lighting_models.h"
#include "shadowenum.h"

shared float4x4 cmWorldView			: REG_cmWorldView;
shared float4x4 cmWorldMat			: REG_cmWorldMat;
//shared float4x4 cmWorldMatTranspose	: REG_cmWorldMatTranspose;
shared float4   cvTextureOffset		: REG_cvTextureOffset;

shared float4	cvLocalLightVec		: REG_cvLocalLightVec;
//shared float4	cvDiffuseColour		: REG_cvDiffuseColour;
//shared float3	cvLocalEyePos		: REG_cvLocalEyePos;

shared float	cfMipMapBias		: REG_cfMipMapBias;

DECLARE_TEXTURE(DIFFUSE_TEXTURE)				// needed for the PC
sampler DIFFUSE_SAMPLER = sampler_state
{
	ASSIGN_TEXTURE(DIFFUSE_TEXTURE)			   // needed for the PC
	AddressU = WRAP;
	AddressV = WRAP;
	DECLARE_MIPFILTER(LINEAR)
	DECLARE_MAGFILTER(LINEAR)
	DECLARE_MINFILTER(LINEAR)
	
};


struct VS_INPUT
{
	float4 position		: POSITION;
	float4 colour		: COLOR;
	float4 tex			: TEXCOORD0;
	float3 normal		: NORMAL;
};

struct PS_INPUT
{
	float4 position			: POSITION;
	float2 alpha_fog		: COLOR0;		// x  = vertex alpha,	y = fog
	float4 tex_ndotl_shadowfade	: TEXCOORD0;	// xy = diffuse uv's
	float4 shadow_tex		: TEXCOORD1;
};

#include "shadowmap_fx_def.h"

PS_INPUT VertexShaderMain(const VS_INPUT IN)
{
	PS_INPUT OUT;
	float4x4 world_mat_transpose = transpose(cmWorldMat);	

	OUT.position		= world_position(IN.position);
	OUT.tex_ndotl_shadowfade.xy	= IN.tex.xy + cvTextureOffset.xy;
	OUT.tex_ndotl_shadowfade.z	= dot( normalize(IN.normal.xyz), cvLocalLightVec.xyz );

	OUT.alpha_fog.x		= IN.colour.w;
	OUT.alpha_fog.y		= CalcVertexFog(OUT.position);
	OUT.shadow_tex		= VertexShadowTexOptimized( IN.position, OUT.tex_ndotl_shadowfade.z, OUT.tex_ndotl_shadowfade.w );
	//OUT.shadow_tex		= VertexShadowTex( IN.position ); OUT.tex_ndotl_shadowfade.w = 0;
	return OUT;
}


float4 PixelShaderMain(const PS_INPUT IN, uniform int shadowDetail) : COLOR0
{
	//
	// Base Textures
	//
	float4 base = tex2Dbias(DIFFUSE_SAMPLER, float4(IN.tex_ndotl_shadowfade.xy, 0, cfMipMapBias));

	// NOTE:  All Lighting Done In Object Space....

	// Accumulate Lighting
	float4 OUT;

	float diffuse = IN.tex_ndotl_shadowfade.z;

	// Shadow Calc
	float shadow = shadowDetail ? DoShadowOptimized( IN.shadow_tex, diffuse, IN.tex_ndotl_shadowfade.w, shadowDetail ) : 1;
	//float shadow = shadowDetail ? DoShadow( IN.shadow_tex, diffuse, shadowDetail ) : 1;

	OUT.xyz = GranitoLightingModelLite( diffuse, shadow, base.rgb );
	OUT.w = base.w * IN.alpha_fog.x;

	OUT.xyz = CalcFogColour(OUT.xyz, IN.alpha_fog.y);
	OUT.xyz = CompressColourSpace(OUT.xyz);

	return OUT;
}

#endif

