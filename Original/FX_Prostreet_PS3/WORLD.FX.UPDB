float2 GetShadowMapTexCoord( const float4 s_tex )
{
	float2	shadow_tex_c = (0.5 * s_tex.xy / s_tex.w) + 0.5f;

	shadow_tex_c.y = 1.0f - shadow_tex_c.y;

	return shadow_tex_c;
}

