//
// Vertex and Pixel Shader Functionality
//

VtoP WaterVertexShader(const VS_INPUT IN)
{
	VtoP OUT;

	OUT.position			= world_position( IN.position );
//	OUT.normal				= IN.normal;
	float3 view_obj			= cvLocalEyePos.xyz - IN.position.xyz; //mul(cvLocalEyePos.xyz - IN.position, cmWorldMat);
	OUT.world_view			= mul(view_obj, (float3x3)cmWorldMat); 
	OUT.tex_opp_fog.xy		= float2(IN.position.xy * (0.0177) + 1.7 * float2(1.0,0.0) * cfTimeTicker);
	OUT.tex_opp_fog.xy		+= cvTextureOffset.xy;	// This does the uv scrolling
#ifdef ENABLE_FOG
	OUT.tex_opp_fog.w		= CalcVertexFog(OUT.position);
#endif
	// Higher up along the skydome, the more the fog falls-off
	
	OUT.tex_opp_fog.z		=	IN.color.a;
#ifndef DISABLE_SHADOW
	OUT.shadow_tex			= VertexShadowTex( IN.position );
#endif

	return OUT;
}


float4 WaterPixelShader(const VtoP IN, int shadowDetail = ShadowShaderDetail_HIGH)
{

	//
	// Decompress the PCA water normal: The alpha and red channel channels are swapped at source because 
	// DXT5 preserves the alpha more accurately
	// 
	float3 normalIn = float3(0,0,1);
	
	float4 component0 = tex2Dbias( PCA_COMPONENTS0_SAMPLER, float4(IN.tex_opp_fog.xy, 0, -2) ) * 2 - 1;
	float4 component1 = tex2Dbias( PCA_COMPONENTS1_SAMPLER, float4(IN.tex_opp_fog.xy, 0, -2) ) * 2 - 1;

	float3 pcaNormal;
	pcaNormal.x  = cafPcaMean.x + (dot(component0, cavPcaWeightsR[0]) +  dot(component1, cavPcaWeightsR[1]));
	pcaNormal.y  = cafPcaMean.y + (dot(component0, cavPcaWeightsG[0]) +  dot(component1, cavPcaWeightsG[1]));
	pcaNormal.z  = cafPcaMean.z + (dot(component0, cavPcaWeightsB[0]) +  dot(component1, cavPcaWeightsB[1]));

	pcaNormal = pcaNormal / 255;	// Normalize
	pcaNormal = pcaNormal * 2 - 1;	// Convert to -1 to 1 space

	pcaNormal = normalize(pcaNormal.xzy);		// Swap due to y-up v.s. z-up differences
	float3 normal = pcaNormal.xyz;

	//Tone Down Normal Map Height... (more for lake)
#ifdef LAKE
	pcaNormal = normalize(lerp( normalIn, pcaNormal.xyz, 0.047 ));
#else
	pcaNormal = normalize(lerp( normalIn, pcaNormal.xyz, 0.21 ));
#endif
	//
	// Lighting setup
	//
		
	// Sun Direction In World Space (we could make it that water needs to be in world space to get rid of this)
	float3 sun_dir_world = normalize(mul( float4(cvLocalLightVec.xyz,0.0), cmWorldMat).xyz);
	
	float4 OUT;	
	float3 view_world = normalize(IN.world_view);

	float3 refl_vect = reflect(-view_world, float3(pcaNormal.xyz));

	float fresnel = pow(dot(view_world, pcaNormal),0.2);

	// Calculate Lookup Into Spherical Sky Map....
	float2 reflectuv;	

	float rot_angle = (cfSkyDomeRotation) * MY_PIECE_OF_PI / 180 + MY_PIECE_OF_PI;
	
	float2 Ve_rot;	
	Ve_rot.x = cos( rot_angle );
	Ve_rot.y = sin( rot_angle );
	
	float2 Vy_rot;
	Vy_rot.x =  -sin( rot_angle );
	Vy_rot.y = cos( rot_angle );

	float phi = acos(refl_vect.z);
	float V = phi / MY_PIECE_OF_PI;
	refl_vect.z = 0.0;
	refl_vect = normalize(refl_vect);
	float beta = (acos( dot( Ve_rot, refl_vect.xy ) ) / (2 * MY_PIECE_OF_PI));	
	float theta = beta;
	if ( dot( Vy_rot, refl_vect.xy ) &lt; 0.0 )
		theta = 1-beta;

	reflectuv.x = theta;
	reflectuv.y = abs((V+0.5)* 2)-1;

	float4 reflection = tex2D( MISCMAP1_SAMPLER, reflectuv.xy );
	reflection = reflection * (0.28 + pow(reflection.w, 4) * 0.784);

	float3 spec = BlinnPhongSpec( view_world, sun_dir_world,normal.xyz,177 );

	float diffuse = dot(sun_dir_world, pcaNormal.xyz);

#ifdef DISABLE_SHADOW
	float shadow = 1.0f;

#else
	float shadow = DoShadow( IN.shadow_tex, diffuse, shadowDetail );
#endif
	spec = spec * shadow;
	reflection = reflection * (0.3 + 0.7 * shadow);

#if PLAT_PS3
	// There is something screwed up on the PS3 with the fresnel-&gt;world_view vector so ignore it!!
	OUT.xyz = reflection.xyz;
#else
	OUT.xyz = (1-fresnel) * reflection.xyz * 2.70;
#endif

	OUT.xyz += (diffuse * float3(0.075,0.075,0.085)) + normal.y * 0.2;
	OUT.xyz +=  spec * cvWorldSunColour;

	OUT.xyz *=  (1/IN.tex_opp_fog.z); // Adjust for opacity difference... pre-multiply....
	OUT.w = IN.tex_opp_fog.z;

#ifdef ENABLE_FOG
	OUT.xyz = CalcFogColour(OUT.xyz, IN.tex_opp_fog.w);
#endif


//	OUT.xyz = float3(1,0,0);
	OUT.xyz = CompressColourSpace(OUT.xyz);
	return OUT;
}

#endif
