#define WORLDCONSTANT_FX

#include "global.h"
#include "fog.h"

shared float4x4 cmWorldMat			: REG_cmWorldMat;
shared float4x4 cmWorldView			: REG_cmWorldView; //WORLDVIEW

shared float4   cvTextureOffset		: REG_cvTextureOffset;
shared float	cfMipMapBias		: REG_cfMipMapBias;



DECLARE_TEXTURE(DIFFUSE_TEXTURE)	
sampler DIFFUSE_SAMPLER = sampler_state  
{
	ASSIGN_TEXTURE(DIFFUSE_TEXTURE)			// needed for the PC
	AddressU = WRAP;
	AddressV = WRAP;
	DECLARE_MIPFILTER(LINEAR)
	DECLARE_MAGFILTER(LINEAR)
	DECLARE_MINFILTER(LINEAR)
	
};

struct VS_INPUT
{
	float4 position		: POSITION;
	float4 colour		: COLOR;
	float4 tex			: TEXCOORD0;
	float3 normal		: NORMAL;
};

struct PS_INPUT
{
	float4 position			: POSITION;
	float4 tex_alpha_fog	: TEXCOORD0;	// xy = diffuse uv's,	z = vertex alpha,  w = fog
};

struct PS_INPUT_DEPTH
{
	float4 position : POSITION;
	float  dist		: COLOR0;
};

#include "shadowmap_fx_def.h"

PS_INPUT VertexShaderMain(const VS_INPUT IN)
{
	PS_INPUT OUT;

	OUT.position			= world_position(IN.position);
	OUT.tex_alpha_fog.xy	= IN.tex.xy + cvTextureOffset.xy;
	OUT.tex_alpha_fog.z		= IN.colour.w;
	OUT.tex_alpha_fog.w		= CalcVertexFog(OUT.position);

	return OUT;
}

struct HEADLIGHT_OUT
{
	float3 diffuse;
};


float4 PixelShaderMain(const PS_INPUT IN) : COLOR0
{

	//
	// Base Textures
	//
	float4 OUT	= tex2Dbias(DIFFUSE_SAMPLER, float4(IN.tex_alpha_fog.xy, 0, cfMipMapBias));
	OUT.w		*= IN.tex_alpha_fog.z;
	OUT.xyz		*= cvVertexPowerBrightness.y;
	OUT.xyz		= CalcFogColour(OUT.xyz, IN.tex_alpha_fog.w);
	OUT.xyz		= CompressColourSpace(OUT.xyz);

	return OUT;
}


#endif

