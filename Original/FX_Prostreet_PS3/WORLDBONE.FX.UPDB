PS_INPUT_LISPSM VertexShaderLiSPSM(const VS_INPUT IN)
{
	PS_INPUT_LISPSM OUT;
	
	float4x4 skinMatrix = camBlendMatrices[IN.index.x] * IN.weight.x +
						  camBlendMatrices[IN.index.y] * IN.weight.y +
						  camBlendMatrices[IN.index.z] * IN.weight.z;

	float4 p			= mul(float4(IN.position.xyz, 1), skinMatrix);
	
	OUT.position = mul(p, cmWorldViewProj);

	return OUT;
}

PS_OUTPUT_LISPSM PixelShaderLiSPSM(const PS_INPUT_LISPSM IN)
{
	PS_OUTPUT_LISPSM OUT;

	OUT.color = 0;

	return OUT;
}


