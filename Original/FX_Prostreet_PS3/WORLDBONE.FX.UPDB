//
// Hero/NIS/Trackside Characters
//
// main shader applies to hero and NIS
// lowlod applies to trackside
// inputs:
//		maps:
//			diffuse (used in both world and additional diffuse computations)
//			spec and ambient occlusion in spec_ao_map.g and spec_ao_map.a (spec is used in both spec and reflections)
//			normal
//		dials and colors thru lightmaterials system:  (NOTE:  the basic scales are multiplied in the //Oban/Speed/Indep/Src/Render/RenderStateCommands.cpp file)
//			base texture blend rgb with world lighting:		DiffuseMaxScale, DiffuseMaxRGB (cvDiffuseMin + cvDiffuseRange)
//			additional diffuse intensity and rgb:			DiffuseMinScale, DiffuseMinRGB (cvDiffuseMin)
//			incidence power, intensity, rgb, bias direction az/el: DiffuseFlakes, SpecularMaxScale, SpecularMaxRGB, EnvmapMinScale, EnvmapMinRB (cvFlakes.x, .., (cvSpecularMin + cvSpecularRange), .., cvEnvmapMinRB)
//			specular power, rolloff, intensity and rgb:		SpecularPower, SpecularFlakes, SpecularMinScale, SpecularMinRGB (cvPowers.y, cvFlakes.y, .., cvSpecularMin)
//			blinn/phong specular intensity, az/el offset:	DiffusePower, EnvmapMaxRB,  (cvPowers.x, cvEnvmapRange.xz)
//			environment map power, intensity:				EnvmapPower, EnvmapVinylScale ( cvPowers.z, cvVinylScales.z)
//			kicker light intensity							DiffuseVinylScale (cvVinylScales.x)
//			(NOTE: kicker light power is the same as the specular power above)

#ifndef	WORLDBONE_H
#define WORLDBONE_H

#include "global.h"
#include "shadowenum.h"

/*** Cross platform declarations *********************************************************************************/

float4x4 cmWorldView			: REG_cmWorldView; //WORLDVIEW
float4x4 cmWorldMat				: REG_cmWorldMat;
float4x4	cmWorldMatTranspose	: REG_cmWorldMatTranspose;
float3	cvLocalEyePos			: REG_cvLocalEyePos; //LOCALEYEPOS;
float4	cvLocalLightVec			: REG_cvLocalLightVec;
//float4	cvLightDirection		: REG_cvLightDirection; // declared in //Oban/Speed/Xenon/Src/Render/Shaders/shadowmap_fx_def.h
float4x4 camBlendMatrices[16]	: REG_camBlendMatrices; //BLENDMATRICES;

#ifdef PLAT_PS3
// soliving matrix packing order issues 
shared float4x4	cmLocalDirectionMatrix	: REG_cmLocalDirectionMatrix; // LOCALDIRECTIONMATRIX;
shared float4x4	cmLocalColourMatrix	: REG_cmLocalColourMatrix; //LOCALCOLOURMATRIX;
#else
shared float4	cmLocalDirectionMatrix[4]	: REG_cmLocalDirectionMatrix; // LOCALDIRECTIONMATRIX;
shared float4	cmLocalColourMatrix[4]	: REG_cmLocalColourMatrix; //LOCALCOLOURMATRIX;
#endif

float		cfMipMapBias		: REG_cfMipMapBias; //MIPMAPBIAS;

shared float4	cvDiffuseMin			: REG_cvDiffuseMin;				// DIFFUSE COLOR;
shared float4	cvDiffuseRange			: REG_cvDiffuseRange;			// TEXTURE BLENDING COLOR;
shared float4	cvSpecularMin			: REG_cvSpecularMin;			// SPECULAR COLOR;
shared float4	cvSpecularRange			: REG_cvSpecularRange;			// INCIDENCE COLOR;
shared float4	cvEnvmapMin				: REG_cvEnvmapMin;				// INCIDENCE BIAS DIRECTION;
shared float4	cvEnvmapRange			: REG_cvEnvmapRange;			// OFFSET BLINN/FONG SPECULAR;

shared float4	cvPowers				: REG_cvPowers;					// xyzw  = Blinn/Phong Intensity, Specular, Env, Not Used
shared float4	cvClampAndScales		: REG_cvClampAndScales; 		// NOT USED	// xyzw = Diffuse, Envmap Clamp		zw = Diffuse, Envmap Scale
shared float4	cvFlakes				: REG_cvFlakes;					// xy   = Incidence powers, Spec Rolloff
shared float4	cvVinylScales			: REG_cvVinylScales;			// xyz  = kicker lighting, Not Used, Env lighting scale amounts

/*** Cross platform includes *****************************************************************************************/
#ifdef ENABLE_FOG
#include "fog.h"
#endif
#include "lighting_models.h"
#include "shadowmap_fx_def.h"

