struct PS_INPUT_LOWLOD
{
	float4 position		: POSITION;
#ifdef ENABLE_FOG
	float3 tex_fog		: TEXCOORD1;	// xy = tex coord, z = fog
#else
	float2 tex_fog		: TEXCOORD2;	// xy = tex coord
#endif
	float4 vertexCol_diffuse	: TEXCOORD3;
};



PS_INPUT_LOWLOD VertexShaderLowLOD(const VS_INPUT IN)
{

	PS_INPUT_LOWLOD OUT;
	float4x4 skinMatrix = camBlendMatrices[IN.index.x] * IN.weight.x +
		camBlendMatrices[IN.index.y] * IN.weight.y +
		camBlendMatrices[IN.index.z] * IN.weight.z;

	float4 p			= mul(float4(IN.position.xyz, 1), skinMatrix);
	float4 normal		= mul(float4(IN.normal.xyz,   0), skinMatrix);
	//float4 t			= mul(float4(IN.tangent.xyz,  0), skinMatrix);

	OUT.position		= world_position(p);
	OUT.tex_fog.xy		= IN.tex.xy;

	// Fog
#ifdef ENABLE_FOG
	OUT.tex_fog.z		= CalcVertexFog(OUT.position);
#endif

	OUT.vertexCol_diffuse.xyz	= IN.vertexCol.xyz;

	// Diffuse
	float4 lightDir		= normalize(cvLocalLightVec);
	float ndotL			= dot(normal, lightDir);
	OUT.vertexCol_diffuse.w	= saturate(ndotL);

	return OUT;
}

float4 PixelShaderLowLOD(const PS_INPUT_LOWLOD IN, int shadowDetail = ShadowShaderDetail_HIGH)
{
	float4 OUT;

	float4 base		= tex2Dbias(DIFFUSE_SAMPLER, float4(IN.tex_fog.xy, 0, -1));
	OUT.xyz			= GranitoLightingModelLite( IN.vertexCol_diffuse.w, 1, base.rgb).xyz;
	OUT.xyz			*= IN.vertexCol_diffuse.xyz;
	OUT.w			= base.w;

	// Fog
#ifdef ENABLE_FOG
	OUT.xyz			= CalcFogColour(OUT.xyz, IN.tex_fog.z);
#endif	
	OUT.xyz			= CompressColourSpace(OUT.xyz);

	return OUT;
}

float4 PixelShader_2_0(const PS_INPUT_LOWLOD IN,  int shadowDetail = ShadowShaderDetail_HIGH)
