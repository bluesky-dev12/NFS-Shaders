/*** Structures **************************************************************************************/

struct VS_INPUT
{
	float4 position		: POSITION;
	float4 normal		: NORMAL;
	float4 vertexCol	: COLOR;
	float4 tex			: TEXCOORD;
	float4 weight		: BLENDWEIGHT;
	float4 index		: BLENDINDICES;
	float4 tangent		: TANGENT;
};

struct PS_INPUT
{
	float4 position		: POSITION;
	float4 position_object : TEXCOORD0;
	float4 normal		: TEXCOORD1;
	float4 tangent		: TEXCOORD2;
#ifdef ENABLE_FOG
	float3 tex_fog		: TEXCOORD3;	// xy = tex coord, z = fog
#else
	float2 tex_fog		: TEXCOORD3;	// xy = tex coord
#endif
	float4 reflection	: TEXCOORD4;
	float4 vertexCol	: TEXCOORD5;
};

/////////////////////////////////////////////////////////////////////////////////////////
// NIS Character shader
//

PS_INPUT NIS_VS(const VS_INPUT IN)
{
	PS_INPUT OUT;

	float4x4 skinMatrix = camBlendMatrices[IN.index.x] * IN.weight.x +
		camBlendMatrices[IN.index.y] * IN.weight.y +
		camBlendMatrices[IN.index.z] * IN.weight.z;

	float4 p				= mul(float4(IN.position.xyz, 1), skinMatrix);
	float4 normal			= mul(float4(IN.normal.xyz,   0), skinMatrix);
	float4 t				= mul(float4(IN.tangent.xyz,  0), skinMatrix);

	OUT.position			= world_position(p);
	OUT.position_object		= p;
	OUT.tex_fog.xy			= IN.tex.xy;
#ifdef ENABLE_FOG
	OUT.tex_fog.z			= CalcVertexFog(OUT.position);
#endif
	OUT.normal				= normal;
	OUT.tangent				= t;

	// Diffuse
	float4 lightDir			= normalize(cvLocalLightVec);
	float ndotL				= dot(normal, lightDir);

	// Calculate the Reflection vector
	float3	view_vector		= normalize(cvLocalEyePos - IN.position.xyz).xyz;
	float3	reflection		= reflect  (-view_vector, normal.xyz);		// R = 2 * (N.V) * N - V

	//OUT.idotn				= dot( view_vector, normal );
	//OUT.view				= mul(float4( view_vector, 0.0f ), cmWorldMat);
	OUT.reflection			= float4( reflection, 0.0f );

	float view_length		= length(view_vector);
	float vdotn				= dot(view_vector, IN.normal.xyz) / view_length;
	float vdotn_sat			= saturate( vdotn );				// Range = 0.0 .. +1.0

	OUT.vertexCol			= IN.vertexCol;

	return OUT;
}


float4 NIS_PS(const PS_INPUT IN, int shadowDetail = ShadowShaderDetail_HIGH)
{
	float4 OUT;

	float3x3 norm_obj_to_tangent_trans;
	norm_obj_to_tangent_trans[0]	= normalize(IN.tangent).xyz;
	norm_obj_to_tangent_trans[2]	= normalize(IN.normal).xyz;
	norm_obj_to_tangent_trans[1]	= cross( norm_obj_to_tangent_trans[2], norm_obj_to_tangent_trans[0] ) * IN.tangent.w;

	// Sun Direction In World Space
	float3 sun_dir_world = normalize(mul( (float3x3)cmWorldMatTranspose, cvLocalLightVec.xyz).xyz);

	float4 base					= tex2Dbias(DIFFUSE_SAMPLER, float4(IN.tex_fog.xy, 0, cfMipMapBias));
	base						*= IN.vertexCol;
	float4 normal				= tex2Dbias(NORMALMAP_SAMPLER, float4(IN.tex_fog.xy, 0, cfMipMapBias)); // normal map

	float4 spec_ao_map			= tex2Dbias(SPECULAR_SAMPLER, float4(IN.tex_fog.xy, 0, -1)); //

	//convert between unsigned and signed normal map data
	normal.xy					= (normal.xy - 0.5)*2;
	normal.z					= sqrt(1 - dot(normal.xy, normal.xy));

	// Place normal in world space
	//normal.xyz					= mul( normal.xyz, norm_obj_to_tangent_trans );
	//normal.xyz					= mul( float4(normal.xyz,0.0), cmWorldMat).xyz;

	// Place normal in world space (tangent-&gt;object-&gt;world)
	normal.xyz = mul( normal.xyz, norm_obj_to_tangent_trans );
	normal.xyz = mul( (float3x3)cmWorldMatTranspose, normal.xyz).xyz;

	normal.xyz					= normalize(normal.xyz);

	float ndotl					= dot(normal.xyz, sun_dir_world );

	// keep view at constant distance
	float3 view					= cvLocalEyePos - IN.position_object.xyz ;
	float3 lockview				= view/length(view);
	lockview					*= 100;

	float3 view_world			= normalize(mul(lockview, (float3x3)cmWorldMat));
	float3 reflection			= reflect(-normalize(view), normal.xyz );
	reflection					= mul(reflection, (float3x3)cmWorldMat);

	float spec					= spec_ao_map.g;
	float ambOcc				= spec_ao_map.a;
	//ambOcc				+= 0.25;  // this is temp to compensate for AO maps not in right domain.

	// texture blending
	float3 textureblend			= (cvDiffuseMin + cvDiffuseRange).xyz;
	float3 worldlight			= GranitoLightingModel( ndotl, 1, base.rgb, ambOcc, 1 ).xyz * cvVinylScales.y;
	OUT.x						= lerp( worldlight.x ,base.x, textureblend.x) ;
	OUT.y						= lerp( worldlight.y ,base.y, textureblend.y) ;
	OUT.z						= lerp( worldlight.z ,base.z, textureblend.z) ;

	//OUT.xyz						+= localGranitoLightingModel( IN.diffuse, shadow, float3(1,1,1), ambOcc, 1.0 ).xyz;
	//OUT.xyz						+= localGranitoLightingModel( ndotl, shadow, float3(1,1,1), ambOcc, 1.0 ).xyz;

	//
	// Car lights...don't use [0]...using [3] for kicker for now KC
	//

	float3	spec_colour3		= cmLocalColourMatrix[3].xyz;
	float3 r					= normalize(IN.reflection.xyz);
	float	spec_dot3			= saturate( dot( cmLocalDirectionMatrix[3].xyz, r) );
	spec_dot3					= pow( spec_dot3, cvPowers.y );
	spec_colour3				*= spec_dot3;
	spec_colour3				*= spec;
	spec_colour3				*= ambOcc;
	spec_colour3				*= cvSpecularMin.xyz;

	float	spec_az				= cvEnvmapMin.x + cvEnvmapRange.x;
	float	spec_el				= cvEnvmapMin.z + cvEnvmapRange.z;

	float3 spec_bias			= float3(cos(radians(spec_az * 720)), sin(radians(spec_az * 720)),(spec_el*2)-1);
	float3 spec_bias_world		= normalize(mul(spec_bias, (float3x3)cmWorldMat));
	float backside				= saturate((dot(view_world, sun_dir_world))) * cvPowers.x &gt; 0.0? cvPowers.x : 0;

	backside = cvPowers.x;
	float3 fullspec				= BlinnPhongSpecChars( view_world, sun_dir_world, normal.xyz, cvPowers.y*50, spec_bias_world, backside)* cvFlakes.y;
	fullspec 					*= spec;
	fullspec					*= cvWorldSunColour;

	// rim
	float3 incidence_bias		= float3(cos(radians(cvEnvmapMin.x  * 360)), sin(radians(cvEnvmapMin.x  * 360)),(cvEnvmapMin.z*2)-1);
	float3 incidence_world		= normalize(mul(lockview + incidence_bias, (float3x3)cmWorldMat));
	float idotn					= 1 - abs(dot(incidence_world, normal.xyz))/2;

	float3 rim					= pow(idotn, cvFlakes.x) * (cvSpecularMin + cvSpecularRange).xyz;

	// Env map stuff
	//float	envmap_scale		= cvVinylScales.z;
	//float4	envmap_sample		= texCUBE  (ENVIROMAP_SAMPLER,	reflection);
	//envmap_sample.xyz			= DeCompressColourSpace(envmap_sample.xyz);
	//float3	envmap_color		= envmap_scale * envmap_sample.xyz * IN.envmap_power.xyz;

	OUT.xyz						+= 2 * (1-OUT.xyz) * fullspec  * ambOcc; // Avoid specular saturation around hot edges....
	OUT.xyz						+= 2 * (1-OUT.xyz) * spec_colour3  * cvVinylScales.x; // kicker light from lightmaterials lightrig
	OUT.xyz						+= rim * ambOcc * cvWorldSunColour;
	//OUT.xyz						+= (envmap_color * ambOcc * spec) ;

	// Fog
#ifdef ENABLE_FOG
	OUT.xyz						 = CalcFogColour(OUT.xyz, IN.tex_fog.z);
#endif
	// OPACITY
	OUT.w						= base.w;


	//  debug for Arthur
	//	OUT.xyz = float3(0, spec_ao_map.g, 0);  // specular
	//	OUT.xyz = float3(spec_ao_map.a, 0, 0);  // ambient occlusion
	//OUT.xyz = abs(IN.normal.xyz);
	//OUT.xyz = normal.xyz;
	//OUT = tex2Dbias(NORMALMAP_SAMPLER, float4(IN.tex_fog.xy, 0, cfMipMapBias)); // normal map
	//OUT.xyz = tex2Dbias(DIFFUSE_SAMPLER, float4(IN.tex_fog.xy, 0, cfMipMapBias)).xyz;
	//OUT.xyz = tex2Dbias(SPECULAR_SAMPLER, float4(IN.tex_fog.xy, 0, -1)).g;
	//OUT.xyz = tex2Dbias(SPECULAR_SAMPLER, float4(IN.tex_fog.xy, 0, -1)).a;
	//OUT.xyz = base;
	//OUT.xyz = IN.vertexCol;
	//OUT.xyz = spec_ao_map.a + 0.25;//ambOcc;
	//OUT.xyz = spec_ao_map.a;//ambOcc;
	//OUT.xyz = spec_ao_map.g;// spec;
	//OUT.xyz = IN.diffuse;// spec;
	//OUT.xyz = ndotl;// spec;
	//OUT.xyz = worldlight;
	//OUT.xyz = shadow;
	//OUT.xyz = fullspec;
	//OUT.xyz = blinnspec;
	//OUT.xyz = fullspec;
	//OUT.w = volSpec.b;
	//OUT.xyz = saturate(ndotl);
	//OUT.r	= saturate(-ndotl);



	OUT.xyz						= CompressColourSpace(OUT.xyz);
	return OUT;
}

