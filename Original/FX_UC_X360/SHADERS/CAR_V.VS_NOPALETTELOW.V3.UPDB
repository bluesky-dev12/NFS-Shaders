float4 VertexShadowTex( float4 v_position, COMPILETIME_BOOL instanceRender )
{
	v_position.w = 1.0f;
	if(instanceRender)
	{
		return mul( v_position, mWorldInstance );
	}
	else
	{
		return mul( v_position, cmWorldMat );
	}
}

