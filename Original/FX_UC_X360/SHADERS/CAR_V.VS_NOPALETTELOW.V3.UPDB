float CalculateShadowFade(float4 shadowTex)
{
	float2 edgeAttenuation = shadowTex.xy / shadowTex.w;
	edgeAttenuation = saturate(1 - pow(edgeAttenuation,4));
	return (1 - edgeAttenuation.x * edgeAttenuation.y);
}

